#include <stdio.h>
int naveen(int **a[100],int m,int n){
    int j;
    for(int i=0;i<=n;i++)
      {
          if(m==a[i])
          {
              j++;
              
          }
      } 
      return j;
}
int main()
{
    int i,j,m,a[100],n;
    printf("enter the n value ");
    scanf("%d",&n);
    printf("enter ele %d",n);
    for(i=0;i<=n;i++)
    scanf("%d",&a[i]);
    
    printf("enter the m ");
    scanf("%d",&m);
    j=naveen(a[100],m,n);
    printf("%d",j);
    

    return 0;
}
